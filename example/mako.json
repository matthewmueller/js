{
  "entries": [ "index.html", "index.js" ],
  "plugins": [
    [ "mako-stat", [ "html", "js" ] ],
    [ "mako-buffer", "js" ],
    [ "..", { "sourceMaps": true, "root": ".." } ],
    [ "mako-sourcemaps", "js", { "inline": true } ],
    [ "mako-output", [ "html", "js", "map" ] ],
    [ "mako-copy", "html" ],
    [ "mako-write", [ "js", "map" ] ]
  ]
}
